{
  "name": "Cracking The Cryptic Helper",
  "description": "Additional control features for Cracking The Cryptic puzzles",
  "version": "1.0.0",
  "manifest_version": 2,
  "icons": {
    "16": "assets/icons/icon16.png",
    "48": "assets/icons/icon48.png",
    "128": "assets/icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://cracking-the-cryptic.web.app/sudoku/*"],
      "js": ["main.js"]
    }
  ],
  "web_accessible_resources": ["page.js"]
}
